{
	"name": "Split Tabs",
	"version": "1.0",
	"manifest_version": 2,
	"key": "",
	"update_url": "https://raw.githubusercontent.com/sirius16/Chrome-Extensions/master/update.xml",
	"description": "Split Tabs",

	"permissions": [
		"tabs",
		"<all_urls>"

	],

	"background": {
		"scripts": ["jquery-1.11.3.min.js", "background.js"],
		"persistent": true
	},

	"commands": {
		"1": {
			"description": "Split Tabs",
			"suggested_key": {
				"default": "Alt+Shift+X"
			}
		},
		"2": {
			"description": "Encapsulate Tabs",
			"suggested_key": {
				"default": "Ctrl+Shift+X"
			}
		}
	},
	"omnibox": {
		"keyword": "##"
	}

}
